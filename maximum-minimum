d1={}
s1=int(input('enter the size :'))
for i in range(0,s1):
    x=input('enter key : ')
    y=input('enter value : ')
    d1[x]=y
print(d1)
print('maximum value : ',max(d1.values()))
print('maximum key : ',max(d1.keys()))
print('minimum value : ',min(d1.values()))
print('minimum value : ',min(d1.keys()))
